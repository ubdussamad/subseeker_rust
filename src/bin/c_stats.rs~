fn find_dominant_spacer () {
    println!("Hello this is Samad!");
    let sample_seprators [char; 5] = ['a','b','c','d','e'];//['\s','[_]','[-]','[.]','[~]'];
    for i in sample_seprators {
        println! ( " {} " , i );
    }
    
    return;
}
